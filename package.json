{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
"scripts": {
  "start": "ng serve",
  "build": "ng build --configuration production",
  "build:vercel": "./node_modules/.bin/ng build --configuration production",
  "preview": "npx serve dist/portfolio",
  "watch": "ng build --watch --configuration development",
  "test": "ng test",
  "deploy": "npm run build:vercel && vercel --prod --yes"
},
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@types/node": "^20.17.19",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
